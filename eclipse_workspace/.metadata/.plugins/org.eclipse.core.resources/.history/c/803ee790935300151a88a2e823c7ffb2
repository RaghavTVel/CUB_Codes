import http.server
import socketserver

host = ''
port = 8080
size = 1024
    
Handler = http.server.SimpleHTTPRequestHandler

httpd = socketserver.TCPServer((host,port),Handler)

print("Serving at Port " + str(port))

class MyHandler(Handler):
    def do_GET(self):
        print("Inside Do Get")
        self.send_response(200)
        




httpd.serve_forever()