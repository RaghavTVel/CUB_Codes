import http.server
import socketserver

host = ''
port = 8080
size = 1024
   
class Handler(http.server.SimpleHTTPRequestHandler):
    def do_GET(self):
        print("Inside Do Get")
        self.send_response(200)
        self.wfile.write("We have got here")
        
 


httpd = socketserver.TCPServer((host,port),Handler)

print("Serving at Port " + str(port))

httpd.serve_forever()